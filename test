local Players = game:GetService("Players")
local player = Players.LocalPlayer

local InventoryPath =
	player.PlayerGui.GameGui.Screen.Middle.Inventory.Inventory
	.Frame.Items.Items.ScrollingFrame

print("========== INVENTORY DEBUG ==========")

for _, item in ipairs(InventoryPath:GetChildren()) do
	if item:IsA("Frame") or item:IsA("ImageButton") then
		print("ITEM:", item.Name)

		-- Attributes
		for attr, val in pairs(item:GetAttributes()) do
			print("  [Attribute]", attr, "=", val)
		end

		-- Values
		for _, v in ipairs(item:GetDescendants()) do
			if v:IsA("StringValue") or v:IsA("IntValue") then
				print("  [Value]", v.Name, "=", v.Value)
			end

			if v:IsA("TextLabel") or v:IsA("TextButton") then
				print("  [Text]", v.Text)
			end
		end

		print("-----------------------------------")
	end
end
