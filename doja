
local function selectmode()
    local args = {
	"dif_apocalypse"
}
game:GetService("ReplicatedStorage"):WaitForChild("RemoteFunctions"):WaitForChild("PlaceDifficultyVote"):InvokeServer(unpack(args))
end
local function raff1()
    local args = {
	"unit_rafflesia",
	{
		Valid = true,
		PathIndex = 1,
		Position = vector.create(51.551246643066406, -21.75, -54.6295280456543),
		DistanceAlongPath = 5.913198724685674,
		CF = CFrame.new(51.551246643066406, -21.75, -54.6295280456543, 0.7071068286895752, 0, -0.7071067690849304, -0, 1, -0, 0.7071068286895752, 0, 0.7071067690849304),
		Rotation = 180
	}
}
game:GetService("ReplicatedStorage"):WaitForChild("RemoteFunctions"):WaitForChild("PlaceUnit"):InvokeServer(unpack(args))

end
local function raff2()
    local args = {
	"unit_rafflesia",
	{
		Valid = true,
		PathIndex = 2,
		Position = vector.create(-54.302581787109375, -21.75, -53.1189079284668),
		DistanceAlongPath = 8.049594790874286,
		CF = CFrame.new(-54.302581787109375, -21.75, -53.1189079284668, 0.7071068286895752, 0, 0.7071067690849304, -0, 1, -0, -0.7071068286895752, 0, 0.7071067690849304),
		Rotation = 180
	}
}
game:GetService("ReplicatedStorage"):WaitForChild("RemoteFunctions"):WaitForChild("PlaceUnit"):InvokeServer(unpack(args))
end
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local player = game.Players.LocalPlayer

-- ‡∏ü‡∏±‡∏á‡∏Å‡πå‡∏ä‡∏±‡∏ô‡∏£‡∏≠‡∏à‡∏ô Seeds >= target
local function waitUntilSeeds(target)
	local gui = player:WaitForChild("PlayerGui"):WaitForChild("GameGui")
	local seedText = gui.Screen.Bottom.CurrencyDisplay.SeedsDisplay.Title

	local function parse(str)
		local cleaned = str:gsub("[^%d]", "")
		return tonumber(cleaned) or 0
	end

	repeat
		task.wait(0.3)
		-- debug
		print("Seed ‡∏ï‡∏≠‡∏ô‡∏ô‡∏µ‡πâ:", parse(seedText.Text))
	until parse(seedText.Text) >= target
end

function raff3()
	print("üå± ‡πÄ‡∏£‡∏¥‡πà‡∏°‡∏ß‡∏≤‡∏á rafflesia ‡∏ï‡∏±‡∏ß‡∏ó‡∏µ‡πà 3")

	-- 1Ô∏è‚É£ ‡∏ß‡∏≤‡∏á‡∏¢‡∏π‡∏ô‡∏¥‡∏ï rafflesia
	local args = {
		"unit_rafflesia",
		{
			Valid = true,
			PathIndex = 1,
			Position = vector.create(51.909950256347656, -21.75, -54.98823165893555),
			DistanceAlongPath = 5.405915231144293,
			CF = CFrame.new(51.909950256347656, -21.75, -54.98823165893555,
				0.7071068286895752, 0, -0.7071067690849304,
				0, 1, 0,
				0.7071068286895752, 0, 0.7071067690849304),
			Rotation = 180
		}
	}

	local placeRemote = ReplicatedStorage:WaitForChild("RemoteFunctions"):WaitForChild("PlaceUnit")
	local success, err = pcall(function()
		placeRemote:InvokeServer(unpack(args))
	end)
	if not success then
		warn("‚ùå PlaceUnit failed:", err)
		return
	end

	-- 2Ô∏è‚É£ ‡∏£‡∏≠‡πÉ‡∏´‡πâ‡∏¢‡∏π‡∏ô‡∏¥‡∏ï‡πÇ‡∏ú‡∏•‡πà‡∏°‡∏≤‡πÉ‡∏ô workspace ‡πÅ‡∏•‡∏∞‡πÄ‡∏Å‡πá‡∏ö instance
	local entities = workspace:WaitForChild("Map"):WaitForChild("Entities")
	local newUnit = nil

	print("‚è≥ ‡∏£‡∏≠ spawn rafflesia...")
	repeat
		for _, u in pairs(entities:GetChildren()) do
			if u.Name == "unit_rafflesia" and not u:GetAttribute("Checked") then
				u:SetAttribute("Checked", true)
				newUnit = u
				break
			end
		end
		task.wait(0.2)
	until newUnit

	print("‚úÖ ‡πÄ‡∏à‡∏≠ rafflesia ‡∏ï‡∏±‡∏ß‡πÉ‡∏´‡∏°‡πà:", newUnit)

	-- 3Ô∏è‚É£ ‡∏î‡∏∂‡∏á UID/UnitId ‡∏Ç‡∏≠‡∏á‡∏¢‡∏π‡∏ô‡∏¥‡∏ï
	local unitId = newUnit:GetAttribute("UID") or newUnit:GetAttribute("UnitId") or newUnit:GetAttribute("Id")
	if not unitId then
		warn("‚ùå ‡πÑ‡∏°‡πà‡∏û‡∏ö UID/UnitId ‡∏Ç‡∏≠‡∏á raff3")
		return
	end
	print("üî¢ UID ‡∏Ç‡∏≠‡∏á‡∏¢‡∏π‡∏ô‡∏¥‡∏ï:", unitId)

	-- 4Ô∏è‚É£ ‡∏£‡∏≠ Seeds ‚â• 8000
	print("‚è≥ ‡∏£‡∏≠ Seeds ‚â• 8000 ‡∏Å‡πà‡∏≠‡∏ô‡∏≠‡∏±‡∏õ‡πÄ‡∏Å‡∏£‡∏î...")
	waitUntilSeeds(8000)

	-- 5Ô∏è‚É£ ‡∏≠‡∏±‡∏õ‡πÄ‡∏Å‡∏£‡∏î‡∏¢‡∏π‡∏ô‡∏¥‡∏ï
	local upgradeRemote = ReplicatedStorage:WaitForChild("RemoteFunctions"):WaitForChild("UpgradeUnit")
	local ok, err = pcall(function()
		upgradeRemote:InvokeServer(unitId)
	end)
	if not ok then
		warn("‚ùå ‡∏≠‡∏±‡∏õ‡πÄ‡∏Å‡∏£‡∏î‡πÑ‡∏°‡πà‡∏™‡∏≥‡πÄ‡∏£‡πá‡∏à:", err)
	else
		print("‚úÖ ‡∏≠‡∏±‡∏õ‡πÄ‡∏Å‡∏£‡∏î raff3 ‡πÄ‡∏™‡∏£‡πá‡∏à‡πÅ‡∏•‡πâ‡∏ß!")
	end
end
selectmode()
task.wait(6)
raff1()
task.wait(12)
raff2()
task.wait(54)
raff3()
